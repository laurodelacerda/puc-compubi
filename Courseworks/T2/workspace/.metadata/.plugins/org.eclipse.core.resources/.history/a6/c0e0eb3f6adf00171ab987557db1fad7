
package br.pucrio.inf.lac;

public class Coordinates {
	
    private final double latitude;
    private final double longitude;

    public Coordinates(double lat, double lon) {
        latitude = lat;
        longitude = lon;
    }

	public double getLatitude() {
		return latitude;
	}

	public double getLongitude() {
		return longitude;
	}
 
}